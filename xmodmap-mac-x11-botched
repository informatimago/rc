keycode  44 = Return


!------------------------------------------------------------
! Fifth line
!------------------------------------------------------------

keycode  64 = Shift_L

keycode  14 = z               Z         Greek_zeta          Greek_ZETA
keycode  15 = x               X         Greek_xi            Greek_XI
keycode  16 = c               C         Greek_chi           Greek_CHI
keycode  17 = v               V         nabla               radical
keycode  19 = b               B         Greek_beta          Greek_BETA
keycode  53 = n               N         Greek_nu            Greek_NU
keycode  54 = m               M         Greek_mu            Greek_MU
keycode  51 = comma           less      lessthanequal       logicaland
keycode  55 = period          greater   greaterthanequal    logicalor
