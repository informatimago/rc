!
! This is an `xmodmap' input file for the  DasKeyboard 3
! on MacOSX with X11.app, when it's botched.
!
! Bus 008 Device 005: ID 04d9:2013 Holtek Semiconductor, Inc. 
!
! PC 105 keys, wide Delete, wide Enter.
!


!------------------------------------------------------------
! First line
!------------------------------------------------------------

keycode  61 = Escape
          
keycode 130 = F1              XF86_Switch_VT_1  F14 
keycode 128 = F2              XF86_Switch_VT_2  F15
keycode 107 = F3              XF86_Switch_VT_3  F16
keycode 126 = F4              XF86_Switch_VT_4  F17

keycode 104 = F5              XF86_Switch_VT_5  F18
keycode 105 = F6              XF86_Switch_VT_6  F19
keycode 106 = F7              XF86_Switch_VT_7  F20
keycode 108 = F8              XF86_Switch_VT_8  F21

keycode 109 = F9              XF86_Switch_VT_9  F22
keycode 117 = F10             XF86_Switch_VT_10 F23
keycode 111 = F11             XF86_Switch_VT_11 F24
keycode 119 = F12             XF86_Switch_VT_12 F25

keycode 113 = Pause           Break
keycode 115 = Caps_Lock       
keycode 121 = Print           Sys_Req


!------------------------------------------------------------
! Second line
!------------------------------------------------------------

keycode  58 = grave           asciitilde      infinity          variation
keycode  26 = 1               exclam          onesubscript      onesuperior
keycode  27 = 2               at              twosubscript      twosuperior
keycode  28 = 3               numbersign      threesubscript    threesuperior
keycode  29 = 4               dollar          foursubscript     foursuperior
keycode  31 = 5               percent         fivesubscript     fivesuperior
keycode  30 = 6               asciicircum     sixsubscript      sixsuperior
keycode  34 = 7               ampersand       sevensubscript    sevensuperior
keycode  36 = 8               asterisk        eightsubscript    eightsuperior
keycode  33 = 9               parenleft       ninesubscript     ninesuperior
keycode  37 = 0               parenright      zerosubscript     zerosuperior
keycode  35 = minus           underscore      righttack         downtack
keycode  32 = equal           plus            lefttack          uptack
keycode  59 = BackSpace       BackSpace       BackSpace         BackSpace       


!------------------------------------------------------------
! Third line
!------------------------------------------------------------

keycode  56 = Tab             ISO_Left_Tab

keycode  20  = q               Q             Greek_omega     Greek_OMEGA
