keycode  42 = i               I             Greek_iota      Greek_IOTA
keycode  39 = o               O             Greek_omicron   Greek_OMICRON
keycode  43 = p               P             Greek_pi        Greek_PI
keycode  41 = bracketleft     braceleft     guillemotleft   0x1002200
keycode  38 = bracketright    braceright    guillemotright  0x1002203
keycode  50 = backslash       bar           integral        partialderivative


!------------------------------------------------------------
! Fourth line (home row)
!------------------------------------------------------------

keycode  67 = Control_L

keycode   8 = a               A             Greek_alpha     Greek_ALPHA
keycode   9 = s               S             Greek_sigma     Greek_SIGMA
keycode  10 = d               D             Greek_delta     Greek_DELTA
keycode  11 = f               F             Greek_phi       Greek_PHI
